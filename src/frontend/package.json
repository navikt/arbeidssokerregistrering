    "@types/react-redux": "5.0.10",
    "@types/lodash": "4.14.91",
    "autoless": "0.1.7",
    "chai": "4.1.2",
    "copyfiles": "1.2.0",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "enzyme-react-intl": "^1.4.8",
    "less": "2.7.3",
    "npm-run-all": "4.0.2",
    "sinon": "4.1.3"
  }
